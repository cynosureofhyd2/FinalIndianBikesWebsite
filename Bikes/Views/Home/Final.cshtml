    width: 120px;
}

nav ul li {
float: none;
margin: 0;
padding: 0;
}

li {
display: list-item;
text-align: -webkit-match-parent;
}
a:link {
-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

